<beangle xmlns="http://beangle.github.io/schema/framework"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://beangle.github.io/schema/framework http://beangle.github.io/schema/framework-1.0.xsd">

  <ems app-name="trd-webapp"/>
  <cdi>
    <module class="org.openurp.trd.web.action.admin.project.DefaultModule"/>
    <module class="org.openurp.trd.web.action.admin.code.DefaultModule"/>
    <module class="org.openurp.trd.web.action.admin.achievement.DefaultModule"/>
    <module class="org.openurp.trd.web.action.admin.team.DefaultModule"/>
    <module class="org.openurp.trd.web.action.admin.textbook.DefaultModule"/>
    <module class="org.openurp.starter.web.ProjectModule"/>
  </cdi>

  <mvc>
    <profile package="org.openurp.trd.web.action">
      <url path="/" suffix="" style="seo"/>
      <interceptor name="web.Interceptor.hibernate"/>
      <interceptor name="web.Interceptor.security"/>
    </profile>
  </mvc>

</beangle>
